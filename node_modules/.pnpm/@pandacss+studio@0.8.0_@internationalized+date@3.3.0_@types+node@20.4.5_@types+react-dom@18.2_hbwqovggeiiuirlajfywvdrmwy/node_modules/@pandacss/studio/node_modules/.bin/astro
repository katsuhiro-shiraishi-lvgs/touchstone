#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/astro@2.8.4_@types+node@20.4.5/node_modules/astro/node_modules:/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/astro@2.8.4_@types+node@20.4.5/node_modules:/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/astro@2.8.4_@types+node@20.4.5/node_modules/astro/node_modules:/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/astro@2.8.4_@types+node@20.4.5/node_modules:/Users/katsuhiro.shiraishi/app/handson/base/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@2.8.4_@types+node@20.4.5/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@2.8.4_@types+node@20.4.5/node_modules/astro/astro.js" "$@"
fi
